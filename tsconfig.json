{
  "extends": "@daotl/tsconfig/vue.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "node",
      "vite/client",
      "vue/ref-macros",
      "vite-plugin-pages/client",
      "vite-plugin-vue-layouts/client",
      "unplugin-icons/types/vue",
      "element-plus/global"
    ],
    "paths": {
      "~/*": ["src/*"]
    }
  },
  "include": ["**/*", "src/**/*.vue"],
  "exclude": ["dist", "node_modules"]
}
