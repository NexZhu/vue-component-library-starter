    "cypress": "^9.3.1",
    "eslint": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.2",
    "pnpm": "^6.28.0",
    "postcss": "^8.4.5",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.0",
