{
  "name": "@daotl/vue-components",
  "version": "0.0.0-alpha.0",
  "prettier": "@daotl/prettier-config",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/vue-components.umd.js",
  "module": "./dist/vue-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-components.es.js",
      "require": "./dist/vue-components.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "vite build",
    "pub": "npm run lint && npm run build && npm publish --access public",
    "build-demo": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint '**/*.{vue,ts,js}'",
    "eslint:fix": "eslint '**/*.{vue,ts,js}' --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,vue,tsx}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss,vue,tsx}' --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
