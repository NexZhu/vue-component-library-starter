{
  "name": "@daotl/vue-components",
  "type": "module",
  "version": "0.0.0-alpha.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/vue-components.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/vue-components.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/vue-components.cjs",
  "module": "./dist/vue-components.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "storybook": "storybook dev",
    "storybook:build": "storybook build",
    "build": "vite build && gcp './dist/types/**/*.d.ts' {p}{n}.cts",
    "build:demo": "vite-ssg build",
    "dev": "dotenv -e .env.dev -- vite --port 3333 --open",
    "pub": "npm run lint && npm run build && npm publish --access public",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unhead/vue": "^1.8.8",
    "@unocss/reset": "^0.57.7",
    "@vueuse/core": "^10.6.1",
    "@vueuse/head": "^2.0.0",
    "element-plus": "^2.4.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.9",
    "vue-demi": "^0.14.6",
    "vue-i18n": "^9.7.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@daotl/eslint-config": "^2.0.0-beta.1",
