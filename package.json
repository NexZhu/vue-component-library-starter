{
  "name": "@daotl/vue-components",
  "version": "0.0.0-alpha.0",
  "private": true,
  "packageManager": "pnpm@7.9.0",
  "prettier": "@daotl/prettier-config",
  "main": "./dist/vue-components.js",
  "module": "./dist/vue-components.mjs",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-components.mjs",
      "require": "./dist/vue-components.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "vite build",
    "pub": "npm run lint && npm run build && npm publish --access public",
    "build-demo": "vite-ssg build",
    "dev": "vite --port 3333 --open",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "prepare": "husky install"
  },
